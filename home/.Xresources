#define FG #b9b9b9
#define BG #161616
#define BL #443a36
#define WH #867564
#define R #7c7c7c
#define G #8e8e8e
#define Y #a0a0a0
#define B #686868
#define M #747474
#define C #868686

URxvt.clipboard.autocopy: true
URxvt.keysym.M-c: perl:clipboard:copy
URxvt.keysym.M-v: perl:clipboard:paste
URxvt.font: xft:FiraCode:size=13
URxvt.internalBorder: 25
URxvt.scrollBar: false
URxvt.letterSpace: 0

! special
*.foreground:   FG
*.background:   BG
*.cursorColor:  FG

! black
*.color0:       BG
*.color8:       BG

! red
*.color1:       R
*.color9:       R

! green
*.color2:       G
*.color10:      G

! yellow
*.color3:       Y
*.color11:      Y

! blue
*.color4:       B
*.color12:      B

! magenta
*.color5:       M
*.color13:      M

! cyan
*.color6:       C
*.color14:      C

! white
*.color7:       FG
*.color15:      FG

! spicetify variables

! foreground
*.color16: #f2f2f2
! background
*.color17: #161616
! foreground secondary
*.color18: #707070
! background secondary
*.color19: #111111
! solid
*.color20: #FFF

! dmenu variables

dmenu.font: FiraCode
dmenu.background: #161616
dmenu.foreground: #707070
dmenu.selbackground: #161616
dmenu.selforeground: #f2f2f2
